{
  "name": "Blockchain Wallet",
  "short_name": "Blockchain",
  "description": "Blockchain Crypto Wallet",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "/img/favicons-b0919a0b68/favicon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "all_frames": true
    }
  ],
  "content_security_policy": {
    "extension_scripts": "default-src 'self'; script-src 'self'"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [ "injected.js" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "key": "oglcnddafdppkfghbjgfbgmmmdompgmp"
}
